{application, fastlog,
 [
  {description, ""},
  {vsn, "1"},
  {registered, []},
  {applications, [
                  kernel,
                  stdlib
                 ]},
  {mod, { fastlog_app, []}},
  {env, [
      {logdir, "log"},
      {log_level, debug},
      {show_in_stdio,true},
      {new_dir_on_start, true},
      {overload_message_queue_len, 20000}
  ]}
 ]}.
